{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 20971520
    }
  },
  "Jwt": {
    "Key": "",
    "Issuer": "",
    "Audience": "",
    "LifetimeMinutes": 43200
  },
  "AlphaVantage": {
    "Quota": {
      "MaxSymbolsPerRun": 8,
      "RequestsPerMinute": 4
    }
  },
  "FileLogging": {
    "PathFormat": "logs/app-{date}.log",
    "RetainedFileCountLimit": 30,
    "FileSizeLimitBytes": 10485760,
    "RollOnFileSizeLimit": true,
    "Append": true,
    "IncludeScopes": false,
    "UseUtcTimestamp": false
  }
}
