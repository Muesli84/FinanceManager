{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 20971520
    }
  },
  "Jwt": {
    "Key": "",
    "Issuer": "",
    "Audience": "",
    "LifetimeMinutes": 43200
  },
  "AlphaVantage": {
    "Quota": {
      "MaxSymbolsPerRun": 8,
      "RequestsPerMinute": 4
    }
  },
  "BackgroundTasks": {
    "Enabled": true
  },
  "Workers": {
    "SecurityPriceWorker": {
      "Enabled": true
    }
  },
  "FileLogging": {
    "Enabled": false,
    "PathFormat": "logs/app-{date}.log",
    "RetainedFileCountLimit": 30,
    "FileSizeLimitBytes": 10485760,
    "RollOnFileSizeLimit": true,
    "Append": true,
    "IncludeScopes": false,
    "UseUtcTimestamp": false
  },
  "Identity": {
    "Lockout": {
      "DefaultLockoutMinutes": 5,
      "MaxFailedAccessAttempts": 3,
      "AllowedForNewUsers": true
    },
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireNonAlphanumeric": false
    }
  }
}
